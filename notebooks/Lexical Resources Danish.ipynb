    "# DanNet http://wordnet.dk/\n",
    "from nltk.corpus import wordnet as wn \n",
    "'dan' in wn.langs()\n",
    "# Returns a list of 'Synsets'\n",
    "# A 'synset' is a set of synonyms that share a common meaning\n",
    "synsets = wn.synsets('mand', lang='dan')\n",
    "print(synsets)\n",
    "synset = synsets[0]\n",
    "print(synset)"
